{
  "version": 2,
  "builds": [
    {
      "src": "packages/site/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/site/$1"
    }
  ],
  "functions": {
    "packages/site/app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  }
}
